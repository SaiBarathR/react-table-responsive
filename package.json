{
  "name": "react-table-responsive-sb",
  "version": "0.1.1",
  "private": false,
  "scripts": {
    "concat:css": "concat -o src/styles/combined.css src/styles/chakraTable.css src/styles/tailwind.css ",
    "build:css": "npm run concat:css && postcss src/styles/combined.css -o dist/styles.css",
    "build": "tsc && npm run build:css"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "chakra-ui-table",
    "mui-table",
    "react-table",
    "responsive-table",
    "react-table-responsive",
    "filter-table",
    "pagination-table",
    "react-pagination-table"
  ],
  "author": "Sai Barath R",
  "license": "ISC",
  "description": "React Table Responsive is a library that provides a responsive table with pagination, search, and filter functionality.",
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.10"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "concat-cli": "^4.0.0",
    "postcss-cli": "^11.0.0",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaiBarathR/react-table-responsive.git"
  }
}